# Gemini API
GEMINI_API_KEY=your-api-key-here
GEMINI_API_VERSION=v1

# Server
PORT=3000
HOST=0.0.0.0
NODE_ENV=development

# Rate Limiting
RATE_LIMIT_WINDOW_MS=3600000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info

# CORS
CHROME_EXTENSION_ID=your-extension-id-here

# Image Processing
MAX_IMAGE_SIZE=5242880
